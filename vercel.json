{
    "version": 2,
    "builds":[
        {
            "src": "login.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ],
    "public": {
        "staticRoutes": {
          "/images": "images",
          "/pages": "pages",
          "/assets": "assets"
        }
      }
}