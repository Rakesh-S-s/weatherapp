{
    "version": 2,
    "builds":[
        {
            "src": "login.js",
            "use": "@vercel/node"
        },
    {
      "src": "views/**/*.ejs",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "views"
      }
    }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"/login.js"
        }
    ]
}