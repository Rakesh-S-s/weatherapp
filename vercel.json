{
    "version": 2,
    "builds":[
        {
            "src": "login.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ],
    "public": {
        "/images": { "source": "images" },
        "/public": { "source": "public" },
        "/pages": { "source": "pages" },
        "/assets": { "source": "assets" }
      }
}